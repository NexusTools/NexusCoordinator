<CoordinatorLibrary>
	<Description>Provides an easy to use modular HTTP server.</Description>
	<Modules>
		<Module class="HTTPCore" />
	</Modules>
	<DefaultConfig>
		<Hosts>
			<Host name="Default" root="/var/www" />
		</Hosts>
	</DefaultConfig>
	<ControlInterface>
		<TabbedWidget>
			<Tabs>
				<Tab name="Statistics" />
				<Tab name="Virtual Hosts" />
				<Tab name="Statistics" />
			</Tabs>
		</TabbedWidget>
	</ControlInterface>
</CoordinatorLibrary>
