<NexusCoordinator>
	<Services>
		<Service class="ControlServer">
			<Protocols>
				<Protocol class="TelnetController" />
				<Protocol class="LocalSocketController" />
			</Protocols>
		</Service>
		<Service class="HTTPServer">
			<Hosts>
				<Host port="2034" processor="HTTPServerManagment" />
			</Hosts>
		</Service>
	</Services>
	<Modules>
		<Module class="ServerManagment" />
		<Module class="HTTPServerManagment" />
	</Modules>
</NexusCoordinator>
